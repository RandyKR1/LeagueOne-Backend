{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.league.com/leagueNew.schema.json",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 50
      },
      "password": {
        "type": "string",
        "minLength": 1
      },
      "maxTeams": {
        "type": "integer",
        "minimum": 1
      },
      "description": {
        "type": "string",
        "minLength": 0
      }
    },
    "additionalProperties": false,
    "required": [
      "name"
    ]
  }
  