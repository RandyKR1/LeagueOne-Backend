{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.match.com/matchNew.schema.json",
  "type": "object",
  "properties": {
    "leagueId": {
      "type": "integer",
      "minimum": 1
    },
    "eventType": {
      "type": "string",
      "enum": ["Friendly", "League", "Tournament", "Final"]
    },
    "eventLocation": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "participant1": {
      "type": "integer",
      "minimum": 1
    },
    "participant2": {
      "type": "integer",
      "minimum": 1
    },
    "participant1Score": {
      "type": "integer",
      "minimum": 0
    },
    "participant2Score": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": [
    "leagueId",
    "eventType",
    "eventLocation",
    "participant1",
    "participant2",
    "participant1Score",
    "participant2Score"
  ],
  "additionalProperties": true
}
