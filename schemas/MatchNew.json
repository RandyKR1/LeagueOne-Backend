{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.match.com/matchNew.schema.json",
    "type": "object",
    "properties": {
      "leagueId": {
        "type": "integer",
        "minimum": 1
      },
      "eventType": {
        "type": "string",
        "minLength": 1,
        "maxLength": 50
      },
      "eventLocation": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "eventCompetition": {
        "type": "string",
        "enum": ["Soccer", "Football", "Hockey", "Basketball", "Tennis", "Golf", "Baseball", "Other"]
      },
      "eventParticipants": {
        "type": "string"
      },
      "eventDate": {
      "type": "string"
      },
      "eventResults": {
        "type": "string",
        "minLength": 0
      }
    },
    "additionalProperties": false,
    "required": [
      "leagueId",
      "eventDate",
      "eventLocation",
      "eventType",
      "eventCompetition"
    ]
  }
  