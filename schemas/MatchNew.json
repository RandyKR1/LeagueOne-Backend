{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.match.com/matchNew.schema.json",
    "type": "object",
    "properties": {
      "leagueId": {
        "type": "integer",
        "minimum": 1
      },
      "eventName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "eventLocation": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "eventType": {
        "type": "string",
        "minLength": 1,
        "maxLength": 50
      },
      "eventResults": {
        "type": "string",
        "minLength": 0
      },
      "creatorId": {
        "type": "integer",
        "minimum": 1
      }
    },
    "additionalProperties": false,
    "required": [
      "leagueId",
      "eventName",
      "eventLocation",
      "eventType",
      "creatorId"
    ]
  }
  