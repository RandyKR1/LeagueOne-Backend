{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.app.com/userUpdate.schema.json",
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "lastName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "email": {
        "type": "string"
      },
      "username": {
        "type": "string",
        "minLength": 3,
        "maxLength": 30
      },
      "password": {
        "type": "string",
        "minLength": 6
      },
      "bio": {
        "type": "string",
        "maxLength": 255
      }
    },
    "additionalProperties": true,
    "required": [
    "firstName", 
    "lastName", 
    "email", 
    "password"
  ]
  }
  